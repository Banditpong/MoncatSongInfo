{
  "name": "MoncatSongInfo Extension",
  "description": "Gets Song Info",
  "manifest_version": 3,
  "version": "1.0","permissions": ["storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.monstercat.app/*"],
      "js": ["background.js"]
    }
  ],
  "action" : {
      "default_popup" : "popup.html"
  },
  "options_page": "options.html"
}
